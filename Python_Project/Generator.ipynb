   "source": [
    "def my_generator():\n",
    "    \"\"\"Simple generator function\"\"\"\n",
    "    a=1\n",
    "    print(\"first time\")\n",
    "    yield a\n",
    "    a+=1\n",
    "    print(\"second time\")\n",
    "    yield a\n",
    "    a+=1\n",
    "    print(\"third time\")\n",
    "    yield a\n",
    "\n",
    "n=my_generator()\n",
    "next(n)"
   ]
